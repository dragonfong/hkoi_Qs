#include <iostream>
#include <string.h>
#include <vector>
#include <algorithm>
#include <fstream>
#include <cstdlib>
#include <time.h>
using namespace std;

int main() {
	
	fstream in;
	fstream out;
	srand(time(NULL));
	for(int a=0;a<1;++a){
		for(int b=0;b<=9;++b){
			string aa=std::to_string(a),bb=std::to_string(b);
			string in_name="input"+aa+bb+".txt";
			string out_name="output"+aa+bb+".txt";
			
			in.open(in_name,ios::out);
			int n=rand()%9+2;										//NNNNNNNNNNNNNNN
			in<<n<<endl;
			for(int c=0;c<n;++c){
				//int sj=rand()%9+2;									//SJSJSJSJSJSJSJSSJ
				int sj=2;
				in<<sj<<" ";
				for(int d=0;d<sj;++d){
					int ele=rand();
					in<<ele<<" ";
				}
				in<<endl;
			}
			in.close();
			
			in.open(in_name,ios::in);
			vector <int> final;
			int N;
			in>>N;
			
			int buffer1;
			in>>buffer1;
			int arr1[buffer1];
			for(int a=0;a<buffer1;++a){
				in>>arr1[a];
			}
			int buffer2;
			in>>buffer2;
			int arr2[buffer2];
			for(int a=0;a<buffer2;++a){
				in>>arr2[a];
			}
			sort (arr1,arr1+buffer1);
			sort (arr2,arr2+buffer2);
			merge (arr1,arr1+buffer1,arr2,arr2+buffer2,final.begin());
			
			for(int q=0;q<n-2;++q){
				int buffer1;
				in>>buffer1;
				int arr1[buffer1];
				for(int a=0;a<buffer1;++a){
					in>>arr1[a];
				}
				sort (arr1,arr1+buffer1);
				merge (arr1,arr1+buffer1,final,final.end(),final.begin());
				
			}
			
			in.close();
			
		}
	}
	
	return 0;
}
